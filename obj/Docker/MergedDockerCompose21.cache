      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: http://*:44355
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: c:\.nuget\fallbackpackages;c:\.nuget\fallbackpackages2
