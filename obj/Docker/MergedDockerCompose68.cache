networks:
  default:
    external:
      name: nat
services:
  app.api:
    build:
      context: D:\LIGHUEN\GIT\Dealership
      dockerfile: App\App.Api\Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:5150
    expose:
    - '5150'
    image: appapi
    ports:
    - published: 5150
      target: 5150
    volumes:
    - C:\Users\USUARIO\AppData\Roaming\ASP.NET\Https:C:\Users\ContainerUser\AppData\Roaming\ASP.NET\Https:ro
    - C:\Users\USUARIO\AppData\Roaming\Microsoft\UserSecrets:C:\Users\ContainerUser\AppData\Roaming\Microsoft\UserSecrets:ro
  app.web:
    build:
      context: D:\LIGHUEN\GIT\Dealership
      dockerfile: App\App.Web\Dockerfile
