networks:
  default:
    external:
      name: nat
services:
  app.api:
    build:
      context: D:\LIGHUEN\GIT\Dealership
      dockerfile: App\App.Api\Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: http://+:5000
    expose:
    - '5000'
    image: appapi
    ports:
    - published: 5150
      target: 5000
    volumes:
    - C:\Users\USUARIO\AppData\Roaming\ASP.NET\Https:C:\Users\ContainerUser\AppData\Roaming\ASP.NET\Https:ro
    - C:\Users\USUARIO\AppData\Roaming\Microsoft\UserSecrets:C:\Users\ContainerUser\AppData\Roaming\Microsoft\UserSecrets:ro
  app.web:
    build:
      context: D:\LIGHUEN\GIT\Dealership
      dockerfile: App\App.Web\Dockerfile
    depends_on:
    - app.api
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: http://+:80
      AssetServiceEndpoint: http://app.api
    expose:
    - '80'
    image: appweb
    ports:
    - published: 5155
      target: 80
    volumes:
    - C:\Users\USUARIO\AppData\Roaming\ASP.NET\Https:C:\Users\ContainerUser\AppData\Roaming\ASP.NET\Https:ro
    - C:\Users\USUARIO\AppData\Roaming\Microsoft\UserSecrets:C:\Users\ContainerUser\AppData\Roaming\Microsoft\UserSecrets:ro
version: '3.4'